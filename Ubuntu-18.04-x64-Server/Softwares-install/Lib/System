#!/bin/bash

function GetUser() {
	echo -e $USER
}

function GetShell() {
	echo -e $SHELL
}

function GetSession() {
	echo -e $SESSION
}


function GetSessionType() {
	echo -e $SESSIONTYPE
}

function GetStartTime() {
	echo -e $(who --boot)
}

function GetTimeActivity() {
	echo -e $(uptime -p)
}

function GetNumberUsers() {
	echo -e $(who -qH)
}

